                output0.dynamicLightmapUV = uv;
            #endif

            output0.positionCS = TransformObjectToHClip(pos);

            return output0;
        }

        [maxvertexcount(12)] //Make a maximum vertex output
        void geom(triangle geomData input[3], inout TriangleStream<geomData> triStream, uint triangleID: SV_PrimitiveID)
        {
            geomData vert0 = input[0];
            geomData vert1 = input[1];
            geomData vert2 = input[2];
            
            int tempSeed = _seed + triangleID;
            for(int i = 0; i < _brushDensity; i++)
            {
                // Compute point on the plane by linear combination of triangle vertices
                // Vector3 P = A + pointDensityA * (B - A) + pointDensityB * (C - A); 
                float pointDensityA = RandomFloatRange(tempSeed, 0.0, 1.0);
                tempSeed = LinearCongruentialMethod(tempSeed);

                float pointDensityB = RandomFloatRange(tempSeed, 0.0, 1.0);
                tempSeed = LinearCongruentialMethod(tempSeed);
      
                // Ensure the points are inside the triangle (barycentric coordinate system)
                if (pointDensityA + pointDensityB > 1)
                {
                    pointDensityA = 1 - pointDensityA;
                    pointDensityB = 1 - pointDensityB;
                }

                float3 P = vert0.positionWS + pointDensityA * (vert1.positionWS - vert0.positionWS) + pointDensityB * (vert2.positionWS - vert0.positionWS);
                P = mul(unity_WorldToObject, float4(P, 1.0)).xyz;
                // Average the normals and add to the spawnNormals list
                float3 N = normalize(cross(vert1.positionWS - vert0.positionWS, vert2.positionWS - vert0.positionWS));

                float3 up = float3(0, 1, 0);
            
                if(abs(dot(up, N)) > 0.95)
                {
                up = float3(1,0,0);
                }
            
                //float rand1 = RandomFloatRange(tempSeed, 0, 3.14159265359);
                //tempSeed = LinearCongruentialMethod(tempSeed);

                //float rand2 = RandomFloatRange(tempSeed, -1, 1);
                //tempSeed = LinearCongruentialMethod(tempSeed);

                //float3 rotationOffset = RandomFloatRange(tempSeed, -_rotation, _rotation);
                //tempSeed = LinearCongruentialMethod(tempSeed);

                float3 a = normalize(cross(N, up));
                //a = normalize(a + rotationOffset);
                float3 b = normalize(cross(N, a));
            
                float3 zOffset = N * RandomFloatRange(tempSeed, -0.01, 0.01);
                tempSeed = LinearCongruentialMethod(tempSeed);

                if(_XRatio == 0)
                {
                    _XRatio = 1;
                }
                if(_YRatio == 0)
                {
                    _YRatio = 1;
                }

                float planeSizeX = _planeSize * _XRatio / (_XRatio + _YRatio);
                float planeSizeY = _planeSize * _YRatio / (_XRatio + _YRatio);

                float3 pos0 = P - (a * planeSizeX + b * planeSizeY) * _planeSize + zOffset;
                float3 pos1 = P - (a * planeSizeX - b * planeSizeY) * _planeSize + zOffset;
                float3 pos2 = P + (a * planeSizeX + b * planeSizeY) * _planeSize + zOffset;
                float3 pos3 = P + (a * planeSizeX - b * planeSizeY) * _planeSize + zOffset;

                geomData output0 = AssignGeomData(pos0, N, float2(0,1));
                geomData output1 = AssignGeomData(pos1, N, float2(0,0));
                geomData output2 = AssignGeomData(pos2, N, float2(1,0));
                geomData output3 = AssignGeomData(pos3, N, float2(1,1));

                triStream.Append(output0);
                triStream.Append(output3);
                triStream.Append(output2);

                triStream.RestartStrip();

                triStream.Append(output2);
                triStream.Append(output1);
                triStream.Append(output0);

                triStream.RestartStrip();
            }

        }
